<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>AlternateItinerary</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>JAVA WS</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>com.alternateItinerary</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>JAVA WS</servlet-name>
    <url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ContactServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.ContactServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContactServlet</servlet-name>
    <url-pattern>/contact</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AccountServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.AccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountServlet</servlet-name>
    <url-pattern>/account</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.EditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditServlet</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CityServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.CityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CityServlet</servlet-name>
    <url-pattern>/city</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CustomCityServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.CustomCityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomCityServlet</servlet-name>
    <url-pattern>/customcity</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CustomRiskServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.CustomRiskServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomRiskServlet</servlet-name>
    <url-pattern>/customrisk</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SimilarityCustomizationServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.SimilarityCustomizationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SimilarityCustomizationServlet</servlet-name>
    <url-pattern>/similaritycustomization</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RiskCustomizationServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.RiskCustomizationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RiskCustomizationServlet</servlet-name>
    <url-pattern>/riskcustomization</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ThresholdServlet</servlet-name>
    <servlet-class>com.alternateItinerary.Controller.ThresholdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ThresholdServlet</servlet-name>
    <url-pattern>/threshold</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>myfilter</filter-name>
    <filter-class>com.alternateItinerary.Controller.myfilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>myfilter</filter-name>
    <url-pattern>/home</url-pattern>
    <url-pattern>/contact</url-pattern>
    <url-pattern>/account</url-pattern>
    <url-pattern>/main</url-pattern>
    <url-pattern>/edit</url-pattern>
    <url-pattern>/logout</url-pattern>
    <url-pattern>/city</url-pattern>
    <url-pattern>/customcity</url-pattern>
    <url-pattern>/customrisk</url-pattern>
    <url-pattern>/similaritycustomization</url-pattern>
    <url-pattern>/riskcustomization</url-pattern>
    <url-pattern>/threshold</url-pattern>
  </filter-mapping>
</web-app>